        status = "okay";
};

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog_1>;

	hog {
		pinctrl_hog_1: hoggrp-1 {
			fsl,pins = <
				MX6QDL_PAD_EIM_D22__GPIO3_IO22		0x80000000	/* otg power en */
				MX6QDL_PAD_GPIO_2__GPIO1_IO02		0x80000000	/* USDHC1 CD */
				MX6QDL_PAD_EIM_DA9__GPIO3_IO09		0x80000000	/* uSDHC3 CD */
				MX6QDL_PAD_GPIO_0__CCM_CLKO1		0x000b0		/* SGTL5000 sys_mclk */
				MX6QDL_PAD_GPIO_6__GPIO1_IO06		0x0b0b0		/* ov5640 mipi powerdown */
				MX6QDL_PAD_KEY_COL4__GPIO4_IO14		0x000b0		/* ov5640 mipi reset */
				MX6QDL_PAD_GPIO_3__CCM_CLKO2		0x000b0		/* ov5640 mclk */
				MX6QDL_PAD_EIM_A25__GPIO5_IO02		0x0b0b0		/* Wifi reset (active low) */
				MX6QDL_PAD_ENET_RXD1__GPIO1_IO26	0x000b0		/* Wifi power enable */
				MX6QDL_PAD_ENET_TXD1__GPIO1_IO29	0x80000000	/* Wifi host wake */
				MX6QDL_PAD_EIM_EB1__GPIO2_IO29		0x000b0		/* Wifi 32kHz slow clock */
				MX6QDL_PAD_EIM_DA11__GPIO3_IO11 	0x80000000  	/* GPIO EDM p256 */
				MX6QDL_PAD_EIM_D27__GPIO3_IO27 		0x80000000 	/* GPIO EDM p258 */
				MX6QDL_PAD_EIM_BCLK__GPIO6_IO31		0x80000000	/* GPIO EDM p260 */
				MX6QDL_PAD_ENET_RX_ER__GPIO1_IO24	0x80000000	/* GPIO EDM p262 */
				MX6QDL_PAD_SD3_RST__GPIO7_IO08 		0x80000000	/* GPIO EDM p264 */
				MX6QDL_PAD_EIM_D26__GPIO3_IO26 		0x80000000	/* GPIO EDM p259 */
				MX6QDL_PAD_EIM_DA8__GPIO3_IO08 		0x80000000	/* GPIO EDM p261 */
				MX6QDL_PAD_GPIO_19__GPIO4_IO05 		0x80000000	/* GPIO EDM p263 */
			>;
		};
	};
};


